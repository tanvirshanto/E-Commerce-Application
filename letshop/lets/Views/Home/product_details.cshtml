                            <input type="password" class="form-control" id="password" name="pass" required>
                        </div>
                        <div class="text-center">
                          <button type="submit" class="btn btn-primary">
                                <i class="fa fa-sign-in"></i>Log in</button> 
                               
                        </div>
                       </form>

