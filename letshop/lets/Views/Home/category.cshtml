                    <div class="row products">

                @foreach (product post in Model.PostBasic)
                {
                     
                        <div class="col-md-3 col-sm-4">
                            <div class="product">
                                <div class="flip-container">
                                    <div class="flipper">
                                        <div class="front">
                                            <a href="detail.html">
                                             <img src="@Url.Content("~/Content/img/" + post.subcat_name + "/"+System.IO.Path.GetFileName(post.img_loc))" alt="" class="img-responsive" style="width:300px;height:250px;" >
                                             
                                            </a>
                                        </div>
                                        <div class="back">
                                            <a href="detail.html">
                                           
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <a href="detail.html" class="invisible">
                                    <img src="img/product1.jpg" alt="" class="img-responsive">
                                </a>
                                <div class="text">
                                    <h3><a href="detail.html">@post.description</a></h3>
                                    <p class="price">@post.price</p>
                                    <p class="buttons">
                                     @Html.ActionLink("View detail", "product_details", new { id = @post.product_id, u = @post.product_id,v=@post.subcat_name},new { @class="btn btn-default" })
                                       <!-- <a href="/Home/product_details" class="btn btn-default">View detail</a> !-->
                                        <a href="basket.html" class="btn btn-primary"><i class="fa fa-shopping-cart"></i>Add to cart</a>
                                    </p>
                                </div>
                                <!-- /.text -->
                            </div>
                            <!-- /.product -->
                        </div>
						
					 
		
                 
