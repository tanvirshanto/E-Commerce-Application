                <div class="col-md-9" id="basket">
                    <div class="box">
                       
                        <h1>
                            Shopping cart</h1>
                        <p class="text-muted">
                            You currently have @Session["count"] item(s) in your cart.</p>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            Product
                                        </th>
                                        <th>
                                           Description
                                        </th>
                                        <th>
                                            Quantity
                                        </th>
                                        <th>
                                            Unit price
                                        </th>
                                        <th>
                                            Discount
                                        </th>
                                        <th colspan="2">
                                            Total
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                @{
                double grandTotal = 0;
               
            }  
                                    @foreach (var item in (List<product>)Session["cart"])
                                    {

                                        double subTotal = Convert.ToDouble(item.quantity) * Convert.ToDouble(item.price);
                                        grandTotal += subTotal;
                                        <tr>
                                            <td>
                                                <a href="#">
                                                    <img src="@Url.Content("~/Content/img/" + item.subcat_name + "/" + System.IO.Path.GetFileName(item.img_loc))" alt="" class="img-responsive" style="width:40px;height:60px;" >
                                                </a>
                                            </td>
                                            <td>
                                                <a href="#">@item.description</a>
                                            </td>
                                            <td>
                                               @using (Html.BeginForm("update", "Home"))
                                               { 
                                                     <input type="number" value="@item.quantity" name="quantity" />
                                                   <input type="hidden" value="@item.product_id" name="product_id" />
                                               
                                                    <input type="image" src="../../Content/img/images.png"/ style="width:20px;height:20px">
                                               
                                               }
                                            </td>
                                          
                                    
                                            <td>
                                                Tk/=@item.price
                                            </td>
                                            <td>
                                                0.00
                                            </td>
                                            <td>
                                                Tk/=@subTotal
                                            </td>
                                            <td>
                                                @Html.ActionLink("Remove", "remove_cart", new { id = @item.product_id }, new { @class = "fa fa-trash-o" })
                                            </td>
                                        </tr>
                                                }
                                </tbody>
                                <tfoot>
                                   <tr>
                                                <th colspan="5">Total</th>
                                                <th>@grandTotal</th>
                                            </tr>
                                            
                                </tfoot>
                            </table>
                        </div>
                        <!-- /.table-responsive -->
                        <div class="box-footer">
                            <div class="pull-left">
                                @Html.ActionLink("Continue shopping", "category", new { id = Request["v"] }, new { @class = "btn btn-primary" })
                            </div>
                            <div class="pull-right">
                                <button class="btn btn-default">
                                    <i class="fa fa-refresh"></i>Update basket</button>
                                @Html.ActionLink("Proceed to checkout", "checkout", new { id = "" }, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                        
                    </div>
                    <!-- /.box -->
                    
                            <!-- /.product -->
                        </div>
                    </div>
